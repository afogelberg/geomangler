{
  "name": "geomangler",
  "version": "1.0.0",
  "description": "Geomangler is a utility library to process and transform geodata, especially useful to prepare data for the web",
  "main": "geomangler.js",
  "scripts": {
    "build": "babel node_modules/ol-es6 --out-dir node_modules/ol --source-maps",
    "build-ol": "node node_modules/openlayers/tasks/build.js tasks/build-ol.json node_modules/openlayers/dist/ol.js"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ]
  },
  "author": "Andreas Fogelberg",
  "license": "MIT",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "bluebird": "^3.5.0",
    "csvjson": "^4.3.4",
    "json2csv": "^3.11.2",
    "jsonfile": "^2.4.0",
    "ol": "^4.3.4",
    "proj4": "^2.3.15",
    "request": "^2.75.0",
    "request-promise": "^4.2.1",
    "source-map-support": "^0.4.18",
    "topojson": "^2.0.0",
    "xml2js": "^0.4.17",
    "xmlbuilder": "^9.0.4"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015-node5": "^1.2.0"
  }
}
