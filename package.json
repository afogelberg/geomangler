{
  "name": "geomangler",
  "version": "1.0.1",
  "description": "Geomangler is a utility library to process and transform geodata",
  "keywords": [
    "mapping",
    "openlayers",
    "geodata",
    "manglers",
    "geojson",
    "topojson"
  ],
  "main": "geomangler.js",
  "engines": {
    "node": ">=6.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/afogelberg/geomangler.git"
  },
  "scripts": {
    "build-ol": "babel node_modules/ol --out-dir node_modules/ol"
  },
  "babel": {
    "presets": [
      "es2015-node6"
    ],
    "plugins": [
      "add-module-exports"
    ]
  },
  "author": "Andreas Fogelberg",
  "license": "MIT",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "bluebird": "^3.5.0",
    "csvjson": "^4.3.4",
    "json2csv": "^3.11.2",
    "jsonfile": "^2.4.0",
    "ol": "^4.4.1",
    "proj4": "^2.3.15",
    "request": "^2.75.0",
    "request-promise": "^4.2.1",
    "source-map-support": "^0.4.18",
    "topojson": "^2.0.0",
    "xml2js": "^0.4.17",
    "xmlbuilder": "^9.0.4"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015-node6": "^0.4.0"
  }
}
